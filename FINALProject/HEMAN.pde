void fourStar(float centerX, float centerY, float size) {
  fill(255);
  stroke(0);
  strokeWeight(1);
  beginShape();
  vertex(centerX - 5*size, centerY);
  vertex(centerX - 1.5*size, centerY - 1.5*size);
  vertex(centerX, centerY - 7.5*size);
  vertex(centerX + 1.5*size, centerY - 1.5* size);
  vertex(centerX + 5*size, centerY);
  vertex(centerX + 1.5*size, centerY + 1.5*size);
  vertex(centerX, centerY + 7.5*size);
  vertex(centerX - 1.5*size, centerY + 1.5* size);
  endShape(CLOSE);
}
